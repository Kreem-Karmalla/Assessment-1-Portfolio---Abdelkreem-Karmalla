days_in_month = {1: 31, 2: 28, 3: 30, 4: 31, 5: 30, 6: 31, 7:30, 8: 31, 9: 30, 10: 31, 11: 30, 12: 31}
print("select month number: ") 
month = int(input())
if 1 <= month <= 12:
    print(f"This month number has {days_in_month[month]} days.")
else:
    print("Invalid number")





